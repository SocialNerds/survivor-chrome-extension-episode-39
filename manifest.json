{
  "name": "SurvivorKiller",
  "description": "Hide Greek TV show 'Survivor' posts form Facebook.",
  "version": "1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "options_page": "options.html",
      "js": [
        "script.js"
      ]
    }
  ]
}
