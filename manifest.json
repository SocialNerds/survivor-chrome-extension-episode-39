{
  "name": "SurvivorKiller",
  "description": "Hide Greek TV show 'Survivor' posts form Facebook.",
  "version": "2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
